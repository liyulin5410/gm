{
  "appenders": [
    {
      "type": "console"
    },
    {
      "type": "file",
      "filename": "${opts:base}/logs/${opts:serverName}.log",
      "pattern": "connector",
      "maxLogSize": 10485760,
      "layout": {
        "type": "basic"
      },
      "backups": 5,
      "category": "test"
    }
  ],
  "levels": {
    "test": "DEBUG"
  },
  "replaceConsole": true,
  "lineDebug": false
}
